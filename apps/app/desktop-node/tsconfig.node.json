{
  "include": [
    "rspack.config.ts",
    "tsconfig.main.json",
    "../../../config"
  ],
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "baseUrl": ".",
    "composite": true,
    "isolatedModules": true,
    "noEmit": false,
    "noEmitOnError": true,
    "rootDir": "../../../",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
