{
  "include": [
    "rspack.config.ts",
    "../../../config"
  ],
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "composite": true,
    "isolatedModules": true,
    "noEmitOnError": true,
    "rootDir": "../../../",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "exclude": ["node_modules"]
}
