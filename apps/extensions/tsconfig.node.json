{
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "rootDir": "../../../",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": [
      "../../config/@types"
    ]
  },
  "include": [
    "tsconfig.web.json",
    "rspack.config.ts",
    "../../config"
  ],
  "exclude": ["node_modules"]
}
