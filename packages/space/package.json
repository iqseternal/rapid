{
  "name": "SPACE",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "suey",
  "homepage": "https://www.oupro.cn",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "start": "electron-vite preview",
    "autodev": "ts-node ./start.ts",
    "dev:app": "electron-vite dev --watch",
    "dev": "electron-vite dev",
    "scripts:dev": "pnpm run -C scripts dev",
    "scripts:build": "pnpm run -C scripts build",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "build": "pnpm typecheck && pnpm scripts:build && electron-vite build",
    "build:win": "pnpm build && electron-builder --win --config",
    "build:mac": "pnpm build && electron-builder --mac --config",
    "build:linux": "pnpm build && electron-builder --linux --config",
    "build:all": "pnpm build && electron-builder --win --config && electron-builder --mac --config && electron-builder --linux --config",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@element-plus/icons-vue": "^2.1.0",
    "@formkit/auto-animate": "^0.8.0",
    "@meta2d/activity-diagram": "^1.0.0",
    "@meta2d/chart-diagram": "^1.0.7",
    "@meta2d/class-diagram": "^1.0.1",
    "@meta2d/core": "^1.0.36",
    "@meta2d/flow-diagram": "^1.0.0",
    "@meta2d/form-diagram": "^1.0.13",
    "@meta2d/fta-diagram": "^1.0.0",
    "@meta2d/le5le-charts": "^1.0.4",
    "@meta2d/sequence-diagram": "^1.0.0",
    "@meta2d/svg": "^1.0.4",
    "@rapid/libs": "workspace:^",
    "@rapid/validates": "workspace:^",
    "animate.css": "^4.1.1",
    "ant-design-vue": "4.x",
    "axios": "^1.5.0",
    "bufferutil": "^4.0.7",
    "dayjs": "^1.11.9",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.3.0",
    "fake-progress": "^1.0.4",
    "file-saver": "^2.0.5",
    "http": "0.0.1-security",
    "lodash": "^4.17.21",
    "marked": "^11.1.1",
    "modules": "link:modules",
    "mousetrap": "^1.6.5",
    "nightmare": "^3.0.2",
    "pako": "^2.1.0",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.2.1",
    "selenium-webdriver": "^4.11.1",
    "utf-8-validate": "^5.0.10",
    "vue-pick-colors": "^1.6.3",
    "vue3-menus": "^1.1.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@crawlee/cheerio": "^3.7.2",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@electron/remote": "^2.0.10",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.199",
    "@types/mousetrap": "^1.6.11",
    "@types/nightmare": "^2.10.6",
    "@types/pako": "^2.0.1",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "ajv": "^7",
    "concurrently": "^8.2.2",
    "crawlee": "^3.5.1",
    "cross-spawn": "^7.0.3",
    "depcheck": "^1.4.3",
    "electron": "^24.6.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "rollup-obfuscator": "^4.1.0",
    "simple-git": "^3.21.0",
    "ts-node": "^10.9.1",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-svg-loader": "^4.0.0",
    "vitepress": "1.0.0-rc.40",
    "vue-tsc": "^1.8.4"
  },
  "lint-staged": {
    "*.{vue,ts,jsx,tsx,json}": "eslint",
    "*.md": [
      "prettier --write"
    ]
  }
}
