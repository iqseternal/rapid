{
  "include": [
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "incremental": true,
    "jsx": "react-jsx",
    "outDir": "../../.rd-cache/libs-web",
    "declarationDir": "../../.rd-cache/libs-web",
    "baseUrl": ".",
    "composite": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "allowUnusedLabels": true,
    "target": "ESNext",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "types": ["./types"]
  },
  "exclude": [
    "node_modules",
    "**/*.test.ts"
  ]
}
