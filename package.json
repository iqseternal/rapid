  },
  "peerDependenciesMeta": {},
  "overrides": {},
  "engineStrict": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.9.0"
  },
  "packageManager": "pnpm@9.15.2",
  "os": [
    "win32",
    "linux"
  ],
  "private": true,
  "lint-staged": {
    "*.{ts,jsx,tsx,json}": "eslint --no-ignore --cache",
    "*.md": [
      "prettier --write"
    ]
  }
}
