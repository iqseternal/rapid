{
  "name": "rapid",
  "version": "1.0.0",
  "description": "Monorepo for Electron Program",
  "private": true,
  "main": "./out/main/index.js",
  "packageManager": "pnpm@8.9.0",
  "scripts": {
    "bt": "tsup",
    "pr": "ts-node ./rsbuild.config.ts",
    "rs": "electron ./packages/desktop-node/src/index.ts",
    "useUtf8": "chcp 65001",
    "typecheck": "pnpm -r --filter=./packages/* run typecheck",
    "start": "pnpm useUtf8 && electron-vite preview",
    "dev": "concurrently \"pnpm dev:web\" \"pnpm dev:desktop\"",
    "dev:app": "pnpm useUtf8 && electron-vite dev --watch",
    "dev:desktop": "pnpm useUtf8 && electron-vite dev",
    "dev:web": "pnpm useUtf8 && pnpm -C ./packages/desktop-web run dev",
    "dev:web:build": "pnpm useUtf8 && pnpm -C ./packages/desktop-web run builda",
    "website:dev": "pnpm -C ./packages/website run dev",
    "website:build": "pnpm -C ./packages/website run builda",
    "docs:dev": "pnpm -C ./docs run dev",
    "docs:build": "pnpm -C ./docs run build",
    "docs:preview": "pnpm -C ./docs run preview",
    "scripts:dev": "pnpm -C scripts dev",
    "scripts:build": "pnpm -C scripts builda",
    "packages:build": "pnpm -r --filter=./packages/* run builda",
    "build": "pnpm typecheck && pnpm scripts:build && pnpm packages:build && pnpm build:only",
    "build:only": "electron-vite builda",
    "build:win": "pnpm builda && electron-builder --win --config",
    "build:win:only": "pnpm build:only && electron-builder --win --config",
    "build:mac": "pnpm builda && electron-builder --mac --config",
    "build:linux": "pnpm builda && electron-builder --linux --config",
    "build:all": "pnpm builda && electron-builder --win --config && electron-builder --mac --config && electron-builder --linux --config",
    "put:website": "pnpm website:build && pnpm -C ./put run put:website",
    "put:docs": "pnpm docs:build && pnpm -C ./put run put:docs",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "electron",
    "vite",
    "meta2d",
    "monorepo",
    "workspace",
    "electron-store",
    "zustand",
    "axios"
  ],
  "homepage": "https://www.oupro.cn",
  "author": {
    "name": "suey",
    "email": "sueyeternal@163.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iqseternal/rapid.git",
    "directory": ""
  },
  "devDependencies": {
    "@babel/plugin-transform-arrow-functions": "^7.24.1",
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/notarize": "^2.3.0",
    "@rsbuild/core": "1.0.1-beta.11",
    "@rsbuild/plugin-check-syntax": "^1.0.1",
    "@rsbuild/plugin-less": "1.0.1-beta.11",
    "@rsbuild/plugin-react": "1.0.1-beta.11",
    "@rsbuild/plugin-sass": "1.0.1-beta.11",
    "@rsbuild/plugin-source-build": "^1.0.1",
    "@rsbuild/plugin-styled-components": "1.0.1-beta.11",
    "@rsbuild/plugin-typed-css-modules": "^1.0.2",
    "@rsdoctor/core": "^0.3.11",
    "@rsdoctor/rspack-plugin": "^0.3.11",
    "@rsdoctor/utils": "^0.3.11",
    "@rspack/cli": "^0.7.5",
    "@rspack/core": "^0.7.5",
    "@types/mousetrap": "^1.6.15",
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.1",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "electron": "^24.8.8",
    "electron-builder": "^23.6.0",
    "electron-store": "^8.2.0",
    "electron-vite": "^2.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pkg": "^5.8.1",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "sass": "^1.77.0",
    "terser": "^5.29.1",
    "tree-kill": "^1.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.2.4",
    "typescript": "5.3.3",
    "vite": "^5.3.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-svg-loader": "^5.1.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@suey/pkg-utils": "^0.1.2",
    "@suey/pkg-web": "^0.0.3",
    "@suey/printer": "^1.1.5",
    "@types/pako": "^2.0.3",
    "animate.css": "^4.1.1",
    "antd": "^5.19.2",
    "axios": "^1.6.7",
    "combined-stream": "^1.0.8",
    "crypto-ts": "^1.0.2",
    "dayjs": "^1.11.10",
    "delayed-stream": "^1.0.0",
    "follow-redirects": "^1.15.5",
    "form-data": "^4.0.0",
    "immer": "^10.0.2",
    "jsrsasign": "^11.1.0",
    "mime-db": "^1.52.0",
    "mime-types": "^2.1.35",
    "mousetrap": "^1.6.5",
    "pako": "^2.1.0",
    "proxy-from-env": "^1.1.0",
    "react": "^18.2.0",
    "react-contexify": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-logger": "^1.1.0",
    "react-router-dom": "^6.16.0",
    "react-transition-group": "^4.4.5",
    "reflect-metadata": "^0.2.2",
    "scp2": "^0.5.0",
    "styled-components": "^6.0.9",
    "ts-md5": "^1.3.1",
    "zustand": "^4.5.4"
  },
  "lint-staged": {
    "*.{ts,jsx,tsx,json}": "eslint --no-ignore",
    "*.md": [
      "prettier --write"
    ]
  }
}
