{
  "name": "rapid",
  "version": "1.0.0",
  "description": "Monorepo for Electron Program",
  "keywords": [
    "react",
    "electron",
    "vite",
    "meta2d",
    "monorepo",
    "workspace",
    "electron-store",
    "electron-builder",
    "zustand",
    "axios",
    "rspack",
    "rsbuild",
    "rspress"
  ],
  "homepage": "http://rapid-doc.oupro.cn",
  "bugs": {
    "url": "https://github.com/iqseternal/rapid/issues",
    "email": "sueyeternal@163.com"
  },
  "license": "MIT",
  "author": {
    "name": "suey",
    "email": "sueyeternal@163.com"
  },
  "main": "./apps/app/out/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/iqseternal/rapid.git",
    "directory": ""
  },
  "scripts": {
    "app:dev": "pnpm -C ./apps/app run dev",
    "app:build": "pnpm -C ./apps/app run build",
    "app:preview": "pnpm -C ./apps/app run preview",
    "docs:dev": "pnpm -C ./apps/docs run dev",
    "docs:build": "pnpm -C ./apps/docs run build",
    "docs:preview": "pnpm -C ./apps/docs run preview",
    "useUtf8": "chcp 65001",
    "typecheck": "pnpm -r run typecheck",
    "start": "pnpm useUtf8 && electron-vite preview",
    "dev": "concurrently \"pnpm dev:web\" \"pnpm dev:desktop\"",
    "dev:app": "pnpm useUtf8 && electron-vite dev --watch",
    "dev:desktop": "pnpm useUtf8 && electron-vite dev",
    "dev:web": "pnpm useUtf8 && pnpm -C ./packages/desktop-web run dev",
    "dev:web:build": "pnpm useUtf8 && pnpm -C ./packages/desktop-web run build",
    "website:dev": "pnpm -C ./apps/website run dev",
    "website:build": "pnpm -C ./apps/website run build",
    "scripts:dev": "pnpm -C ./apps/scripts dev --config ./apps/scripts/.npmrc",
    "scripts:build": "pnpm -C ./apps/scripts build --config ./apps/scripts/.npmrc",
    "packages:build": "pnpm -r run build",
    "build": "pnpm typecheck && pnpm packages:build",
    "build:win": "pnpm build && electron-builder --win --config",
    "build:win:only": "pnpm build:only && electron-builder --win --config",
    "build:mac": "pnpm build && electron-builder --mac --config",
    "build:linux": "pnpm build && electron-builder --linux --config",
    "build:all": "pnpm build && electron-builder --win --config && electron-builder --mac --config && electron-builder --linux --config",
    "put:website": "pnpm website:build && pnpm -C ./put run put:website",
    "put:docs": "pnpm docs:build && pnpm -C ./put run put:docs",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --cache",
    "test": "jest -o",
    "test:all": "jest",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "config": {},
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@react-spring/web": "9.7.5",
    "@suey/pkg-web": "0.0.3",
    "@suey/printer": "1.1.11",
    "@types/pako": "2.0.3",
    "animate.css": "4.1.1",
    "animejs": "3.2.2",
    "antd": "5.19.2",
    "archiver": "7.0.1",
    "dayjs": "1.11.10",
    "immer": "10.0.2",
    "jsrsasign": "11.1.0",
    "mousetrap": "1.6.5",
    "pako": "2.1.0",
    "qrcode.react": "4.2.0",
    "rc-menu": "9.15.1",
    "react": "18.3.1",
    "react-advanced-cropper": "0.20.0",
    "react-dom": "18.3.1",
    "react-hot-toast": "2.5.1",
    "react-i18next": "13.2.2",
    "react-logger": "1.1.0",
    "react-player": "2.16.0",
    "react-responsive-masonry": "2.6.0",
    "react-router-dom": "6.16.0",
    "react-spinkit": "3.0.0",
    "react-spinners": "0.15.0",
    "react-transition-group": "4.4.5",
    "reflect-metadata": "0.2.2",
    "styled-components": "6.1.13",
    "swiper": "11.2.1",
    "ts-md5": "1.3.1",
    "unzip": "0.1.11",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-arrow-functions": "7.24.1",
    "@commitlint/cli": "19.1.0",
    "@commitlint/config-conventional": "19.1.0",
    "@electron-toolkit/preload": "3.0.0",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@electron-toolkit/utils": "3.0.0",
    "@electron/notarize": "2.3.0",
    "@rsbuild/core": "1.1.0",
    "@rsbuild/plugin-check-syntax": "1.0.1",
    "@rsbuild/plugin-less": "1.1.0",
    "@rsbuild/plugin-react": "1.1.0",
    "@rsbuild/plugin-sass": "1.1.0",
    "@rsbuild/plugin-source-build": "1.0.1",
    "@rsbuild/plugin-styled-components": "1.1.0",
    "@rsbuild/plugin-typed-css-modules": "1.0.2",
    "@rsdoctor/core": "0.3.11",
    "@rsdoctor/rspack-plugin": "0.3.11",
    "@rsdoctor/utils": "0.3.11",
    "@rspack/cli": "1.1.0",
    "@rspack/core": "1.1.0",
    "@swc/core": "1.7.12",
    "@types/archiver": "6.0.3",
    "@types/jest": "29.5.12",
    "@types/mousetrap": "1.6.15",
    "@types/node": "20.11.26",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/react-transition-group": "4.4.5",
    "@types/styled-components": "5.1.28",
    "@types/unzip": "0.1.4",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@vitejs/plugin-react": "4.3.1",
    "atomically": "2.0.3",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "electron": "24.8.8",
    "electron-builder": "24.13.3",
    "electron-store": "8.2.0",
    "electron-vite": "2.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "only-allow": "1.2.1",
    "prettier": "3.2.5",
    "prettier-eslint": "16.3.0",
    "rsbuild-plugin-tailwindcss": "0.0.3",
    "rspress": "1.27.1",
    "sass": "1.77.0",
    "scp2": "0.5.0",
    "tailwindcss": "3.4.17",
    "terser": "5.29.1",
    "tree-kill": "1.2.2",
    "ts-jest": "29.2.4",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.8.8",
